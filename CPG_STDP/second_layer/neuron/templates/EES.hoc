begintemplate EES
public connect2target, ees

objref ees

proc init() {

    ees = new NetStim(0.8)
        ees.interval = 25
        ees.start = 1
        ees.number = 1000000000
        ees.noise = 0

}

obfunc connect2target() { localobj nc
    nc = new NetCon(ees, $o1)
    if (numarg() == 2) { $o2 = nc }
    return nc
}

endtemplate EES